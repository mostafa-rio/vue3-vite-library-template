{
  "name": "vue-plugin-template",
  "private": false,
  "version": "0.0.0",
  "type": "module",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mostafa-rio/vue3-vite-library-template.git"
  },
  "main": "./dist/myVuePackge.cjs",
  "module": "./dist/myVuePackge.js",
  "exports": {
    ".": {
      "import": "./dist/myVuePackge.js",
      "require": "./dist/myVuePackge.esm.cjs"
    },
    "./dist/style.css": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    }
  },
  "types": "./dist/index.d.ts",
  "keywords": [
    "vue3-plugin",
    "vue3",
    "vite-config",
    "vite-"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build ",
    "run-example": "cd src/example && vite",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "peerDependencies": {
    "vue": "^3.4.4"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@storybook/addon-essentials": "^7.6.7",
    "@storybook/addon-interactions": "^7.6.7",
    "@storybook/addon-links": "^7.6.7",
    "@storybook/blocks": "^7.6.7",
    "@storybook/test": "^7.6.7",
    "@storybook/vue3": "^7.6.7",
    "@storybook/vue3-vite": "^7.6.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^7.6.7",
    "typescript": "^5.3.3",
    "vite": "^5.0.8",
    "@rollup/plugin-typescript": "^11.1.5",
    "@vitejs/plugin-vue": "^5.0.2",
    "rollup-plugin-dts": "^6.1.0",
    "rollup-plugin-typescript2": "^0.36.0",
    "vite-plugin-css-injected-by-js": "^3.3.1",
    "vite-plugin-dts": "^3.7.0"
  }
}
